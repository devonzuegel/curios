  "proxy": "http://localhost:5000",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": ["prettier --write", "git add"]
  },
  "scripts": {
    "start:dev": "./bin/start-dev",
    "heroku-postbuild": "yarn build",
    "start": "./bin/start-prod",
    "build": "./bin/build",
    "test": "./bin/test",
    "storybook": "start-storybook -p 6006 -c client/.storybook",
    "build-storybook": "build-storybook -c client/.storybook"
  },
  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.3.0",
    "jest-diff": "^22.0.6",
    "nodemon": "^1.13.3"
  },
  "jest": {
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx,ts,tsx}",
      "server/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": ["<rootDir>/client/config/polyfills.js"],
    "testMatch": [
      "<rootDir>/client/src/**/?(*.)test.ts?(x)",
      "<rootDir>/server/**/?(*.)test.ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/client/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/client/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/client/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"],
    "moduleNameMapper": {
      "@client/(.*)": "<rootDir>/client/src/$1",
      "@server/(.*)": "<rootDir>/server/$1",
      "@test/(.*)": "<rootDir>/test/$1",
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": ["react-app"]
  },
  "license": "UNLICENSED",
  "eslintConfig": {
    "extends": "react-app"
  }
}
